:root{--border:1px solid #D4D4D4;--color-default:#FAF9F6;--color-primary:#6A9E9F;--color-primary-opacity-8:rgba(106, 158, 159, 0.08);--color-neutral:#F2F2EF;--color-neutral-darker:#E5E5E2;--color-contrast:#4F4F4F;--color-contrast-opacity-8:rgba(79, 79, 79, 0.08);--color-contrast-opacity-16:rgba(79, 79, 79, 0.16);--color-contrast-opacity-36:rgba(79, 79, 79, 0.36);--color-contrast-opacity-86:rgba(79, 79, 79, 0.86)}body{background-color:var(--color-default)!important;color:var(--color-contrast)!important}.bg-white{background-color:var(--color-default)!important}.bg-black-90{background-color:var(--color-contrast)!important}.bg-near-white{background-color:var(--color-neutral)!important}.dark-gray,.black-90,.near-black{color:var(--color-contrast)!important}.white-90,.white{color:var(--color-default)!important}.gray{color:var(--color-contrast-opacity-86)!important}.mid-gray{color:var(--color-contrast-opacity-36)!important}.site-title,.f1,.f2,.f3,.f4,h1,h2,h3,h4,h5,h6{color:var(--color-contrast)!important}.blue,.link,a{color:var(--color-primary)!important}.blue:hover,.link:hover,a:hover{color:var(--color-primary)!important;opacity:.8}.bg-black-90{background-color:var(--color-contrast)!important}.bg-black-90 .white-90{color:var(--color-default)!important}.mw7.center,article{background-color:var(--color-default)}.bg-light-gray{background-color:var(--color-neutral)!important}.bt,.bb,.bl,.br,.ba{border-color:#d4d4d4!important}.b--light-gray{border-color:var(--color-neutral-darker)!important}input,textarea,select{background-color:var(--color-neutral)!important;border:var(--border)!important;color:var(--color-contrast)!important}input:focus,textarea:focus,select:focus{border-color:var(--color-primary)!important;box-shadow:0 0 0 2px var(--color-primary-opacity-8)!important}.btn,button{background-color:var(--color-primary)!important;color:var(--color-default)!important;border:none!important}.btn:hover,button:hover{background-color:var(--color-primary)!important;opacity:.9}pre,code{background-color:var(--color-neutral)!important;color:var(--color-contrast)!important;border:var(--border)!important}blockquote{border-left:4px solid var(--color-primary)!important;background-color:var(--color-primary-opacity-8)!important;color:var(--color-contrast)!important}table{border-collapse:collapse}th,td{border:var(--border)!important;background-color:var(--color-default)!important;color:var(--color-contrast)!important}th{background-color:var(--color-neutral)!important}tr:nth-child(even){background-color:var(--color-neutral)!important}footer{background-color:var(--color-neutral-darker)!important;color:var(--color-contrast)!important}.pagination a{color:var(--color-primary)!important;border:var(--border)!important}.pagination a:hover{background-color:var(--color-primary-opacity-8)!important}.f6.gray,.gray,.post-meta{color:var(--color-contrast-opacity-86)!important}.tag,.category{background-color:var(--color-primary-opacity-8)!important;color:var(--color-primary)!important;border:1px solid var(--color-primary)!important}.highlight{background-color:var(--color-primary-opacity-8)!important}::selection{background-color:var(--color-primary-opacity-8)!important;color:var(--color-contrast)!important}*:focus{outline:2px solid var(--color-primary)!important;outline-offset:2px!important}@media screen and (max-width:30em){.mw7.center{background-color:var(--color-default)!important}}